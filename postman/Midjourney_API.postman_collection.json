          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"prompt\": \"a beautiful sunset over mountains, photorealistic\"\n}"
        },
        "url": {
          "raw": "{{base_url}}/generate",
          "host": [
            "{{base_url}}"
          ],
          "path": [
            "generate"
          ]
        },
        "description": "Generazione base di un'immagine con solo il prompt"
      },
      "response": []
