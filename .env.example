# Midjourney API Configuration
# Copy this file to .env and fill in your actual values

# Discord Server ID where your Midjourney bot is located
SERVER_ID="your_discord_server_id"

# Discord Channel ID where Midjourney operates
CHANNEL_ID="your_discord_channel_id"

# Discord bot token (Salai token for Midjourney)
SALAI_TOKEN="your_discord_bot_token"

# Port for the API server (default: 3147)
PORT=3147
